<!DOCTYPE html>
<html dir="ltr" lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <!-- Tell the browser to be responsive to screen width -->
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="">
  <meta name="author" content="">

  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag 'application'%>
  <%= stylesheet_link_tag 'admins'%>

  <link rel="stylesheet" href="//cdn.materialdesignicons.com/3.6.95/css/materialdesignicons.min.css">



  <!-- Favicon icon -->
  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
<!--[if lt IE 9]>
<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
<script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
<![endif]-->
</head>

<body>
  <!-- ============================================================== -->
  <!-- Preloader - style you can find in spinners.css -->
  <!-- ============================================================== -->
  <div class="preloader">
    <div class="lds-ripple">
      <div class="lds-pos"></div>
      <div class="lds-pos"></div>
    </div>
  </div>
  <!-- ============================================================== -->
  <!-- Main wrapper - style you can find in pages.scss -->
  <!-- ============================================================== -->
  <div id="main-wrapper">
    <!-- ============================================================== -->
    <!-- Topbar header - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <header class="topbar" data-navbarbg="skin5">
      <nav class="navbar top-navbar navbar-expand-md navbar-dark">
        <div class="navbar-header" data-logobg="skin5">
          <!-- This is for the sidebar toggle which is visible on mobile only -->
          <a class="nav-toggler waves-effect waves-light d-block d-md-none" href="javascript:void(0)"><i class="ti-menu ti-close"></i></a>
          <!-- ============================================================== -->
          <!-- Logo -->
          <!-- ============================================================== -->
          <a class="navbar-brand" href="index.html">
            <!-- Logo icon -->
            <b class="logo-icon p-l-10">
              <!--You can put here icon as well // <i class="wi wi-sunset"></i> //-->
              <!-- Dark Logo icon -->
              <%= image_tag 'logo-icon.png', alt:"homepage", class: "light-logo" %>

            </b>
            <!--End Logo icon -->
            <!-- Logo text -->
            <span class="logo-text">
              <!-- dark Logo text -->
              <%= image_tag 'logo-text.png', alt:"homepage", class: "light-text" %>


            </span>
            <!-- Logo icon -->
            <!-- <b class="logo-icon"> -->
              <!--You can put here icon as well // <i class="wi wi-sunset"></i> //-->
              <!-- Dark Logo icon -->
              <!-- <img src="../../assets/images/logo-text.png" alt="homepage" class="light-logo" /> -->

              <!-- </b> -->
              <!--End Logo icon -->
            </a>
            <!-- ============================================================== -->
            <!-- End Logo -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Toggle which is visible on mobile only -->
            <!-- ============================================================== -->
            <a class="topbartoggler d-block d-md-none waves-effect waves-light" href="javascript:void(0)" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><i class="ti-more"></i></a>
          </div>
          <!-- ============================================================== -->
          <!-- End Logo -->
          <!-- ============================================================== -->
          <div class="navbar-collapse collapse" id="navbarSupportedContent" data-navbarbg="skin5">
            <!-- ============================================================== -->
            <!-- toggle and nav items -->
            <!-- ============================================================== -->
            <ul class="navbar-nav float-left mr-auto">
              <li class="nav-item d-none d-md-block"><a class="nav-link sidebartoggler waves-effect waves-light" href="javascript:void(0)" data-sidebartype="mini-sidebar"><i class="mdi mdi-menu font-24"></i></a></li>
              <!-- ============================================================== -->
              <!-- create new -->
              <!-- ============================================================== -->

              <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                  <span class="d-none d-md-block">Templates <i class="fa fa-angle-down"></i></span>
                  <span class="d-block d-md-none"><i class="fa fa-plus"></i></span>   
                </a>
                <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                  <a class="dropdown-item" href="templates/backend/html/ltr/index.html" target="blank">Template Backend</a>
                  <a class="dropdown-item" href="templates/frontend/dinomuz/index.html" target="blank">Template Frontend</a>
                  <a class="dropdown-item" href="rails/info/routes" target="blank">Routes</a>
                </div>
              </li>
              <!-- ============================================================== -->
              <!-- Search -->
              <!-- ============================================================== -->
              <li class="nav-item search-box"> <a class="nav-link waves-effect waves-dark" href="javascript:void(0)"><i class="ti-search"></i></a>
                <form class="app-search position-absolute">
                  <input type="text" class="form-control" placeholder="Search &amp; enter"> <a class="srh-btn"><i class="ti-close"></i></a>
                </form>
              </li>
            </ul>
            <!-- ============================================================== -->
            <!-- Right side toggle and nav items -->
            <!-- ============================================================== -->
            <ul class="navbar-nav float-right">

              <!-- ============================================================== -->
              <!-- User profile and search -->
              <!-- ============================================================== -->
            </ul>
          </div>
        </nav>
      </header>
      <!-- ============================================================== -->
      <!-- End Topbar header -->
      <!-- ============================================================== -->
      <!-- ============================================================== -->
      <!-- Left Sidebar - style you can find in sidebar.scss  -->
      <!-- ============================================================== -->
      <aside class="left-sidebar" data-sidebarbg="skin5">
        <!-- Sidebar scroll-->
        <div class="scroll-sidebar">
          <!-- Sidebar navigation-->
          <nav class="sidebar-nav">
            <ul id="sidebarnav" class="p-t-30">
              <li class="sidebar-item">
                <%= link_to root_path, class: "sidebar-link waves-effect waves-dark sidebar-link", aria_expanded:"false" do %>
                <i class="mdi mdi-view-dashboard"></i><span class="hide-menu">Dashboard</span>
                <% end %>
              </li>
              <li class="sidebar-item">
                <%= link_to search_lines_path, class: "sidebar-link waves-effect waves-dark sidebar-link", aria_expanded:"false" do %>
                <i class="mdi mdi-search-web"></i><span class="hide-menu">Linhas de pesquisa</span>
                <% end %>
              </li>
              <li class="sidebar-item">
                <%= link_to papers_path, class: "sidebar-link waves-effect waves-dark sidebar-link", aria_expanded:"false" do %>
                <i class="mdi mdi-instapaper"></i><span class="hide-menu">Papers</span>
                <% end %>
              </li>
              <li class="sidebar-item">
                <%= link_to members_path, class: "sidebar-link waves-effect waves-dark sidebar-link", aria_expanded:"false" do %>
                <i class="mdi mdi-account-multiple"></i><span class="hide-menu">Membros</span>
                <% end %>
              </li>
              <li class="sidebar-item">
                <%= link_to disclosures_path, class: "sidebar-link waves-effect waves-dark sidebar-link", aria_expanded:"false" do %>
                <i class="mdi mdi-newspaper"></i><span class="hide-menu">Noticias</span>
                <% end %>
              </li>
              <li class="sidebar-item">
                <%= link_to site_texts_path, class: "sidebar-link waves-effect waves-dark sidebar-link", aria_expanded:"false" do %>
                <i class="mdi mdi-format-text"></i><span class="hide-menu">Textos do site</span>
                <% end %>
              </li>
              <li class="sidebar-item">
                <%= link_to root_path, class: "sidebar-link waves-effect waves-dark sidebar-link", aria_expanded:"false" do %>
                <i class="mdi mdi-earth"></i><span class="hide-menu">Site</span>
                <% end %>
              </li>
              



            </ul>
          </nav>
          <!-- End Sidebar navigation -->
        </div>
        <!-- End Sidebar scroll-->
      </aside>
      <!-- ============================================================== -->
      <!-- End Left Sidebar - style you can find in sidebar.scss  -->
      <!-- ============================================================== -->
      <!-- ============================================================== -->
      <!-- Page wrapper  -->
      <!-- ============================================================== -->
      <div class="page-wrapper">
        <!-- ============================================================== -->
        <!-- Bread crumb and right sidebar toggle -->
        <!-- ============================================================== -->
        <%= yield %>
        

        <!-- ============================================================== -->
        <!-- End Container fluid  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- footer -->
        <!-- ============================================================== -->
        <footer class="footer text-center">
          All Rights Reserved by Matrix-admin. Designed and Developed by <a href="https://wrappixel.com">WrapPixel</a>.
        </footer>
        <!-- ============================================================== -->
        <!-- End footer -->
        <!-- ============================================================== -->
      </div>
      <!-- ============================================================== -->
      <!-- End Page wrapper  -->
      <!-- ============================================================== -->
    </div>
    <!-- ============================================================== -->
    <!-- End Wrapper -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- All Jquery -->
    <!-- ============================================================== -->

    <%= javascript_include_tag 'application'%>
    <%= javascript_include_tag 'admins' %>
    

    <!-- Editor de texto -->
    <script>
      $(function() { $('.textarea').froalaEditor() });
    </script>

    <!-- Popover image-->
    <script>
      $(document).ready(function(){
       $('[data-toggle="popover"]').popover({
          //trigger: 'focus',
          trigger: 'hover',
          html: true,
          content: function () {
            return $(this).data('content');
          },
          title: 'Toolbox'
        }) 
     });
   </script>
   
   <!-- Select2 -->
  <script>    
    $(".select2").select2();
  </script>



 </body>

 </html>